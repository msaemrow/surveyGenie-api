{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/survey.schema.json",
  "type": "object",
  "properties": {
    "user_id": {
      "type": "integer"
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "survey_description": {
      "type": "string"
    },
    "questions": {
      "type": "array",
      "minItems": 1
    }
  },
  "required": ["title", "questions"],
  "additionalProperties": false
}
